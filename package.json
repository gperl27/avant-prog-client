{
  "name": "redux-boilerplate",
  "version": "1.0.0",
  "description": "Grab 'n Go Redux Boilerplate",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --port 3000 --devtool eval --progress --colors --hot --content-base dist",
    "test": "react-scripts test --env=jsdom",
